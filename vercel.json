{
  "buildCommand": "npm install --force && npm run build",
  "framework": "angular",
  "installCommand": "npm install --force",
  "outputDirectory": "dist/studying-angular",
  "devCommand": "ng serve",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "engines": {
    "node": "16.x"
  }
}